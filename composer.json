{
    "name": "ahmedofali/laravel-elastic-search-log",
    "description": "Integrate laravel with elastic search for sending all logs to elastic search ",
    "keywords": [
        "laravel",
        ":elasticsearch",
        ":log"
    ],
    "homepage": "https://github.com/ahmedofali/laravel-elastic-search-log",
    "license": "MIT",
    "authors": [
        {
            "name": ":ahmedofali",
            "email": ":dev.ahmed.abbas@gmail.com",
            "homepage": "https://github.com/ahmedofali/laravel-elastic-search-log",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "elasticsearch/elasticsearch": "^7.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.3",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Ahmedofali\\LaravelElkLog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ahmedofali\\LaravelElkLog\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ahmedofali\\LaravelElkLog\\ElkServiceProvider"
            ],
            "aliases": {
                "Elk": "Ahmedofali\\LaravelElkLog\\ElkFacade"
            }
        }
    }
}
